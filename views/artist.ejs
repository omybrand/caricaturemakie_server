    <!-- Header -->
    <header class="artist_header">
        <div class="container about_artist">
            <img class="artist_pic_big" src="<%=info.artist_photo %>">
            <h5 class="artist_introduce korean"><%=info.artist_ment %></h5>
            <br>
            <h4 class="artist_profile">[artist profile]</h4>
            <p class="artist_info korean">
                <% for (var i in datas) { %>
                    <% if (datas[i].artist_info_month == '현재') { %>
                        <strong>[<%=datas[i].artist_info_year %>~현재]</strong><br>
                    <% } else if (datas[i].artist_info_month.length == 4) { %>
                        <strong>[<%=datas[i].artist_info_year %>~<%=datas[i].artist_info_month %>]</strong><br>
                    <% } else if (datas[i].artist_info_month == '00') { %>
                        <strong>[<%=datas[i].artist_info_year %>]</strong><br>
                    <% } else { %>
                        <strong>[<%=datas[i].artist_info_year %>.<%=datas[i].artist_info_month %>]</strong><br>
                    <% } %>

                    <%- datas[i].artist_info_content %><br>

                    <% if (datas.length - 1 > i) { %>
                        <br>
                    <% } %>
                <% } %>
            </p>
        </div>
    </header>
    <!--/.header end-->
