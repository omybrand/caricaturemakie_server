          function cal() {
            var mode = Number(document.getElementById("mode").value);
            var size = Number(document.getElementById("size").value);
            var pcount = Number(document.getElementById("pcount").value);
            var add1p = Number(document.getElementById("add1p").value);
            var add2p = Number(document.getElementById("add2p").value);
            var add3p = Number(document.getElementById("add3p").value);
            if (pcount == 1) {
                var price = mode + size;
                $('#result').html('캐리커쳐 가격: ' + String(price).comma() + '원 +  @');
            } else if (pcount == 2) {
                var basic = mode + size;
                var price = basic + basic * add1p;
                $('#result').html('캐리커쳐 가격: ' + String(price).comma() + '원 +  @');
            } else if (pcount == 3) {
                var basic = mode + size;
                var price = basic + basic * add1p + basic * add2p;
                $('#result').html('캐리커쳐 가격: ' + String(price).comma() + '원 +  @');
            } else if (pcount == 4) {
                var basic = mode + size;
                var price = basic + basic * add1p + basic * add2p + basic * add3p;
                $('#result').html('캐리커쳐 가격: ' + String(price).comma() + '원 +  @');
            }
          }

          String.prototype.comma = Number.prototype.comma = function() {
              var n = String(this).replace(/\,/g,'');
              return n.match(RegExp('^[0-9]{'+(n.length%3||3)+'}|[0-9]{3}','g')).join();
          };

      </script>
